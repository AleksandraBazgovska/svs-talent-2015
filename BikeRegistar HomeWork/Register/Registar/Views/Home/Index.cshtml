@model IEnumerable<Registar.DomainModel.Bike>

@{
    Layout = "../MasterPage.cshtml";
}




<h3>Bike search result</h3>


    <div class="table">
        <table class="table-bordered">
            <thead>
                <tr>
                    <th>
                        RegNumber
                    </th>
                    <th>
                        Producer
                    </th>
                    <th>
                        Model
                    </th>
                    <th>
                        Colour
                    </th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var bike in this.Model)
                {
                    <tr>
                        <td>@bike.RegNumber</td>
                        <td>@bike.Producer</td>
                        <td>@bike.Model</td>
                        <td>@bike.Colour</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>

